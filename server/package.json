{
  "name": "server",
  "version": "0.0.0",
  "scripts": {
    "build": "node build.js",
    "lint": "tslint --project \"tsconfig.json\"",
    "start": "node -r module-alias/register ./dist --env=production",
    "start:dev": "nodemon",
    "start:db": "nodemon --db=real",
    "test": "nodemon --config ./spec/nodemon.json --db=real",
    "front-test": "node build.js &&  webpack && node -r module-alias/register ./dist --env=production",
    "build-webpack": "webpack --watch"
  },
  "nodemonConfig": {
    "watch": [
      "src"
    ],
    "ext": "ts, html",
    "ignore": [
      "src/public"
    ],
    "exec": "ts-node -r tsconfig-paths/register ./src"
  },
  "_moduleAliases": {
    "@daos": "dist/daos",
    "@entities": "dist/entities",
    "@shared": "dist/shared",
    "@server": "dist/Server",
    "@dist": "dist/type"
  },
  "dependencies": {
    "@types/express-session": "^1.17.0",
    "@types/session-file-store": "^1.2.1",
    "body-parser": "^1.19.0",
    "command-line-args": "^5.1.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.1",
    "helmet": "^3.23.3",
    "html-webpack-plugin": "^4.3.0",
    "http-status-codes": "^1.4.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "mysql2": "^2.1.0",
    "passport": "^0.4.1",
    "passport-local": "^1.0.0",
    "sequelize": "^6.3.3",
    "session-file-store": "^1.4.0",
    "winston": "^3.3.3"
  },
  "devDependencies": {
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@types/command-line-args": "^5.0.0",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.7",
    "@types/find": "^0.2.1",
    "@types/helmet": "0.0.47",
    "@types/jasmine": "^3.5.11",
    "@types/jsonfile": "^6.0.0",
    "@types/morgan": "^1.9.1",
    "@types/mysql2": "github:types/mysql2#semver:^1.0.0",
    "@types/node": "^14.0.23",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-airbnb-typescript": "^8.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^2.5.1",
    "file-loader": "^6.0.0",
    "find": "^0.3.0",
    "fs-extra": "^9.0.1",
    "jasmine": "^3.5.0",
    "jsonfile": "^6.0.1",
    "node-sass": "^4.14.1",
    "nodemon": "^2.0.4",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "supertest": "^4.0.2",
    "ts-loader": "^8.0.0",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.2",
    "typescript": "^3.7.2",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  }
}
